<% provide(:title, "Sign Up") %>
<h1>Sign Up</h1>
<p>This will be a signup page for new users.</p>
<% form_for :users do |f| %>
  Name: <%= f.text_field :name %>
  Email: <%= f.text_field :email %>
  Password: <%= f.password_field :password %>
  Confirmation: <%= f.password_field :confirmPassword %>
  <% f.submit %>
<% end %>
